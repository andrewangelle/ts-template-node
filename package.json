{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "lint:fix": "pnpm exec biome check . --write",
    "lint:check": "pnpm exec biome check .",
    "format": "pnpm exec biome format . --write",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "keywords": [],
  "author": "andrewangelle@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "typescript": "^5.7.2"
  }
}
